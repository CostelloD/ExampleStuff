@page  "{id}"
@model TestExamples.Pages.PersonDetailsModel
@{
    ViewData["Title"] = "PersonDetails";
}

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        // list out each of the properties.
        <tr>
            <td>@Model.Person.ChildFirstNAme</td>
            <td>@Model.Person.DateOfBirth</td>
            <td>@Model.Person.PhoneMobile</td>
            <td>@Model.Person.StartDate</td>

            @*<td> <a asp-page="PersonDetails" asp-route-id="@Model.Person.ChildFirstNAme">Details</a> </td>*@
        </tr>
    </tbody>
</table>

